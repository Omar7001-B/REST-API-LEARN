{
    "version": 2,
    "builds": [
        {
            "src": "back/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "front/",
            "use": "@vercel/static",
            "config": {
                "distDir": "front/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/back/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/front/$1"
        }
    ]
}